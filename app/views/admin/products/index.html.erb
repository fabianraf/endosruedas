
		<h3 class="text-center">Encuentra la bicicleta que tu necesitas</h3>
	<div class="table-responsive">
	  <table class="table ">
	  	<thead>
	  		<tr>
	  			<th>Producto</th>
	  			<th>Tipo</th>
	  			<th>Modelo</th>
	  			<th>Marca</th>
	  			<th>Última Actualización</th>
	  			<th>Acciones</th>
	  		</tr>
	  	</thead>
	  	<tbody>
	  	<% @products.each_with_index do |product, index| %>
		      <%= render :partial => "product", locals: {product: product, index: index} %>
		<% end %>
		
	  	</tbody>
	    
	  </table>
	</div>
	
	<div class="row"> 
		<%= will_paginate(@products) %>
	</div> 
