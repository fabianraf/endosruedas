<tr>
	<td><%= product.title %></td>
	<td><%= product.class.name == "Bicycle" ? "Bicicleta" : "Motocicleta" %></td>
	<td><%= product.model_id %></td>
	<td><%= product.brand_id %></td>
	<td><%= product.updated_at.try(:strftime, "%d-%m-%Y") %></td>
	<td><%= link_to "Ver", admin_producto_path(product) %> | <%= link_to "Editar", edit_admin_producto_path(product) %> | <%= link_to "Activar/Desactivar", edit_admin_producto_path(product) %></td>
</tr>